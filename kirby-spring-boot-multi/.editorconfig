root=true

[*]
indent_style = space
indent_size = 4
continuation_indent_size = 4
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false

[*.java]
max_line_length = 120

[*.sh]
indent_size = 2
continuation_indent_size = 2

[*.bat]
indent_style = tab
end_of_line = crlf
